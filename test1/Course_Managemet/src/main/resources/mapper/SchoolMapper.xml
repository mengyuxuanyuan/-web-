<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.pojo.School">
    <!--select查询语句-->
    <!--id为方法名,resultType为结果返回类型-->
    <select id="findById1" parameterType="int"
            resultType="com.example.demo.pojo.School">
    select * from s_school where id = #{id};
  </select>

</mapper>